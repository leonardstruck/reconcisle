{
  "name": "reconcisle",
  "productName": "reconcisle",
  "version": "0.0.3",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "typings": "./webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [],
  "author": {
    "name": "Leonard Struck",
    "email": "info@leonardstruck.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/icon/icon.icns",
        "osxSign": {
          "hardened-runtime": true,
          "gatekeeper-assess": false,
          "entitlements": "entitlements.plist",
          "entitlements-inherit": "entitlements.plist"
        },
        "osxNotarize": {
          "appleId": "leonard@blockform.at",
          "appleIdPassword": "@keychain:Application Loader: leonard@blockform.at"
        }
      },
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "draft": false,
            "repository": {
              "owner": "leonardstruck",
              "name": "reconcisle"
            }
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "reconcisle"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/pure-render-decorator": "^0.2.28",
    "@types/react": "^17.0.3",
    "@types/react-addons-css-transition-group": "^15.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.0.1",
    "copy-webpack-plugin": "6.4.1",
    "css-loader": "^4.2.1",
    "electron": "12.0.2",
    "electron-builder": "^22.10.5",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.23.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "node-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "ttf-loader": "^1.0.2",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.1"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.42.0",
    "@blueprintjs/popover2": "^0.4.0",
    "@blueprintjs/select": "^3.15.9",
    "ajv": "^8.0.1",
    "ajv-formats": "^2.0.0",
    "electron-debug": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-storage": "^1.0.7",
    "electron-store": "^7.0.2",
    "electron-unhandled": "^3.0.2",
    "mariadb": "^2.5.3",
    "mysql2": "^2.2.5",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0",
    "react-router-dom": "^5.2.0",
    "react-uuid": "^1.0.2",
    "sequelize": "^6.6.2",
    "slugify": "^1.5.0",
    "sqlite3": "^5.0.2",
    "tedious": "^11.0.6",
    "update-electron-app": "^2.0.1"
  }
}
