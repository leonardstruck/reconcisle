{
	"name": "reconcisle",
	"productName": "reconcIsle",
	"version": "0.0.3",
	"description": "My Electron application description",
	"main": ".webpack/main",
	"scripts": {
		"start": "NODE_ENV=development electron-forge start",
		"rebuild": "./node_modules/.bin/electron-rebuild -f",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"make-win": "electron-forge make --platform win32",
		"publish": "electron-forge publish",
		"lint": "echo \"No linting configured\""
	},
	"keywords": [],
	"author": {
		"name": "Leonard Struck",
		"email": "info@leonardstruck.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/leonardstruck/reconcisle.git"
	},
	"license": "MIT",
	"config": {
		"forge": {
			"packagerConfig": {
				"asar": true,
				"icon": "./src/app/assets/icon/icon.icns",
				"app-bundle-id": "com.leonardstruck.reconcisle",
				"osxSign": {
					"hardened-runtime": true,
					"gatekeeper-assess": false,
					"entitlements": "entitlements.plist",
					"entitlements-inherit": "entitlements.plist"
				},
				"osxNotarize": {
					"appleId": "leonard@blockform.at",
					"appleIdPassword": "@keychain:Application Loader: leonard@blockform.at"
				}
			},
			"publishers": [
				{
					"name": "@electron-forge/publisher-github",
					"config": {
						"draft": false,
						"repository": {
							"owner": "leonardstruck",
							"name": "reconcisle"
						}
					}
				}
			],
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "reconcisle"
					}
				},
				{
					"name": "@electron-forge/maker-dmg",
					"config": {
						"format": "ULFO",
						"icon": "./src/app/assets/icon/icon.icns"
					}
				},
				{
					"name": "@electron-forge/maker-zip",
					"platforms": [
						"darwin",
						"linux"
					]
				},
				{
					"name": "@electron-forge/maker-deb",
					"config": {}
				},
				{
					"name": "@electron-forge/maker-rpm",
					"config": {}
				}
			],
			"plugins": [
				[
					"@electron-forge/plugin-auto-unpack-natives"
				],
				[
					"@electron-forge/plugin-webpack",
					{
						"mainConfig": "./webpack.main.config.js",
						"renderer": {
							"config": "./webpack.renderer.config.js",
							"entryPoints": [
								{
									"html": "./src/reconc/index.html",
									"js": "./src/reconc/renderer.js",
									"name": "reconc"
								},
								{
									"html": "./src/index.html",
									"js": "./src/renderer.js",
									"name": "main_window"
								}
							]
						}
					}
				]
			]
		}
	},
	"devDependencies": {
		"@babel/core": "^7.13.14",
		"@babel/preset-react": "^7.13.13",
		"@electron-forge/cli": "^6.0.0-beta.54",
		"@electron-forge/maker-deb": "^6.0.0-beta.54",
		"@electron-forge/maker-rpm": "^6.0.0-beta.54",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.54",
		"@electron-forge/maker-zip": "^6.0.0-beta.54",
		"@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.54",
		"@electron-forge/plugin-webpack": "6.0.0-beta.54",
		"@electron-forge/publisher-github": "^6.0.0-beta.54",
		"@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
		"@svgr/webpack": "^5.5.0",
		"babel-loader": "^8.2.2",
		"css-loader": "^4.2.1",
		"electron": "12.0.2",
		"electron-devtools-installer": "^3.1.1",
		"electron-rebuild": "^2.3.5",
		"node-loader": "^1.0.2",
		"style-loader": "^1.2.1"
	},
	"dependencies": {
		"@blueprintjs/core": "^3.42.0",
		"@blueprintjs/popover2": "^0.4.0",
		"@blueprintjs/select": "^3.15.9",
		"@blueprintjs/table": "^3.8.27",
		"@electron-forge/maker-dmg": "^6.0.0-beta.54",
		"ajv": "^8.0.3",
		"ajv-formats": "^2.0.1",
		"cardinal": "^2.1.1",
		"electron-builder": "^22.10.5",
		"electron-config": "^2.0.0",
		"electron-log": "^4.3.2",
		"electron-squirrel-startup": "^1.0.0",
		"electron-store": "^7.0.2",
		"fs-extra": "^9.1.0",
		"mariadb": "^2.5.3",
		"mysql2": "^2.2.5",
		"pg": "^8.5.1",
		"pg-hstore": "^2.3.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-helmet": "^6.1.0",
		"react-hot-loader": "^4.13.0",
		"react-router-dom": "^5.2.0",
		"react-uuid": "^1.0.2",
		"sequelize": "^6.6.2",
		"slugify": "^1.5.0",
		"sqlite3": "^5.0.2",
		"tedious": "^11.0.6",
		"webpack": "^5.28.0"
	}
}
